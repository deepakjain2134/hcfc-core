@model IEnumerable<HCF.BDO.FrequencyMaster>

@{
    ViewBag.Title = "Frequencies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    @section pageHeader{
    <section class="content-header">
        <h1>
            Frequencies
        </h1>
    </section>
        }
    <div class="container-fluid">
        <div class="row">
            <div class="settingnewBox">

                @await Html.PartialAsync("_MoreConfig")
                @*<div class="col-lg-3 col-xs-3 col-lg-1-8">
                    <div class="box box-primary col-lg-12 general_menu">
                        @await Html.PartialAsync("_MoreConfig")
                    </div>
                    </div>*@
                <div class="col-md-9 col-xs-9 col-lg-8-2">
                    <div class="box box-primary">
                        <div class="add_button_comman">
                            <a href='@(Url.Action("mngfrequencies", "Organization"))' class="pull-right">
                                <img src="@Url.Content(HCF.Web.Models.ImagePathModel.AddIcon)" title="Add Building" alt="3d_add_button" />
                            </a>
                        </div>
                        
                            <table id="myTable" class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th style="display:none"></th>
                                        <th>
                                            Frequency Name
                                        </th>
                                        <th>GracePeriod</th>
                                        <th>
                                            Status
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.OrderBy(x => x.DisplayName))
                                    {
                                        <tr>
                                            <td style="display:none">
                                                @Html.DisplayFor(modelItem => item.FrequencyId)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.DisplayName)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.GracePeriod)
                                            </td>
                                            <td class="comman_checkbox1">
                                                @Html.DisplayFor(modelItem => item.IsActive)<span></span>
                                            </td>
                                            @*<td>*@
                                            @*<img onclick="location.href=@Url.Action("MngFrequencies", "Organization", new { id = item.FrequencyId })"
                                                src="@Url.Content(HCF.Web.Models.ImagePathModel.EditIcon)" title="Edit" />*@
                                            <td class="build-editicon">
                                                <img onclick="location.href ='@Url.Action("MngFrequencies", "Organization", new { id = item.FrequencyId })'" src="@Url.Content(HCF.Web.Models.ImagePathModel.EditIcon)" title="Edit" />
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>


