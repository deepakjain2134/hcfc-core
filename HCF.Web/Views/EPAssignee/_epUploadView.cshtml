@using System.Data;
@model DataTable
@if (Model != null)
{
    <hr />

    @if (@ViewData["nothavingemails"] != null)
    {
        <p>Some of emails is not existing in our database so please review and correct them. @ViewData["nothavingemails"] </p>
    }
    <table border="0" cellpadding="0" cellspacing="0">
        <tr>
            <th>Select</th>
            @foreach (DataColumn dc in Model.Columns)
            {
                <th>@dc.ColumnName</th>
            }
        </tr>
        @foreach (DataRow dr in Model.Rows)
        {
            <tr>
                <td><input type="checkbox" class="epdetailId" value="@dr["EPDetailId"].ToString()" name="epdetailId"><span></span></td>
                @foreach (DataColumn dc in Model.Columns)
                {
                    <td>@dr[dc.ColumnName]</td>
                }
            </tr>
        }
    </table>
}