@model IEnumerable<HCF.BDO.EPDetails>

@{
    ViewBag.Title = "Goal";
}
<h2>epNumber</h2>

    <section class="content-header">
        <h1>
            Goal
        </h1>
    </section>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary">
                    <div class="box-header">
                        <a href='@(Url.RouteUrl("addep"))' class="btn btn-primary pull-right">
                            <span>
                                @Localizer["Add"]
                               
                            </span>
                        </a>
                    </div>
                   
                        <table id="example1" class="table table-bordered table-striped">
                            <tr>
                                <th>
                                    TJCStandard
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.EPNumber)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Frequency.DisplayName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Description)
                                </th>
                                <th></th>
                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Standard.TJCStandard)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EPNumber)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Frequency.DisplayName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Description)
                                    </td>
                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.EPDetailId }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.EPDetailId })
                                    </td>
                                </tr>
                            }
                        </table>
                   
                </div>
            </div>
        </div>
    </div>

