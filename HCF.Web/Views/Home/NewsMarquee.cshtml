@model IEnumerable<KeyValuePair<string, string>>
@{
    ViewBag.Title = "_newsMarquee";
    Layout = null;
}


<marquee onmouseover="this.stop();"
         onmouseout="this.start();"
         direction="left"
         hspace="60"
         scrollamount="5"
         style="float: right;">
    <b>
        @foreach (var item in Model)
        {
            <span>
                <a href="@Url.Action("NewsDescription", "News", new { newsId = item.Key, type = "0" })">@item.Value</a>
            </span>
        }
    </b>
</marquee>