@model List<HCF.BDO.CopDetails>
@{
    /**/

    ViewBag.Title = "CMS Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    @section pageHeader{
        <section class="content-header">
            <h1>
                CMS Dashboard
            </h1>
        </section>
    }
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary">
                    <div class="">
                        <table id="c_myTable" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th width="10%">Requirement</th>
                                    <th width="32%">CoP Text</th>
                                    @*<th style="width: 150px;">CoP Name</th>
                                        <th>Cop Title</th>*@
                                    <th width="8%">Tag Code</th>
                                    <th width="50%">
                                        <table class="table">
                                            <tr>
                                                <th style="width:30%;">@Localizer["StandardEP"]</th>
                                                <th title="@Localizer["InspectionDate"]" style="width:20%;">
                                                    <img alt="@Localizer["InspectionDate"]" src="@Url.Content(HCF.Web.Models.ImagePathModel.CalendarIcon)" title="Next Inspection Date" />
                                                </th>
                                                <th title="@Localizer["Users"]" style="width:20%;">
                                                    <img alt="@Localizer["Users"]" src="@Url.Content("~/dist/Images/Icons/dashboard_user_icon.png")" title="@Localizer["Users"]">
                                                </th>
                                                <th style="width:20%;" title="@Localizer["Frequency"]">
                                                    <img alt="@Localizer["Frequency"]" src="@Url.Content("~/dist/Images/Icons/dashboard_clock.png")" title="@Localizer["Frequency"]" />
                                                </th>
                                                <th style="width:10%;" title="@Localizer["DocRequired"]" class="doc-name">
                                                    <img alt="@Localizer["DocRequired"]" src="@Url.Content("~/dist/Images/Icons/dashboard_doc_icon.png")" title="@Localizer["DocRequired"]" />
                                                </th>
                                            </tr>
                                        </table>
                                    </th>

                                </tr>
                            </thead>
                            <tbody id="partialContents">
                                @await Html.PartialAsync("_cmsDashboard", Model)
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>



