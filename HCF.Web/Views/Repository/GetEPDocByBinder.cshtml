@model List<HCF.BDO.EPsDocument>
@{
    ViewBag.Title = "GetEPDocByBinder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section pageHeader{
    <section class="content-header">
        <h1>
            All Documents
        </h1>
    </section>
}
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
               
                    <div class="search_options">
                    </div>
                    <div>
                        <input type="hidden" name="pageName" id="pageName" value="1" />
                        @await Html.PartialAsync("~/Views/Repository/_getEPDocByBinder.cshtml", Model)
                    </div>
                
            </div>
        </div>
    </div>
</div>
