@model List<HCF.BDO.TRoundUsers>

<div class="adduserListpanel">
    <h4 class="roundsHeading" id="rinspections"><i class="fa fa-chevron-down"></i> Volunteers</h4>
    <ul class="crx-scrollBar vouserhideshow">
        @foreach (var roundUsers in Model.Where(x => x.IsActive || x.InspStatus != -1).OrderBy(y => y.User.FullName).ToList())
        {
            if (@roundUsers.User.UserId != 0)
            {
                <li>
                    @roundUsers.User.FullName

                </li>
            }



        }
    </ul>
</div>
<script>
    $("#rinspections").click(function () {
        $(".vouserhideshow").slideToggle('linear');
        $("#rinspections").find(".fa").toggleClass("rightarrow");
    });
</script>