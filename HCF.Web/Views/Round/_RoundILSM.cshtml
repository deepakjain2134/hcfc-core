@model List<HCF.BDO.TIlsm>

<div class="adduserListpanel">
    <h4 class="roundsHeading" id="rilsm">
        <i class="fa fa-chevron-down"></i> ILSM (@Model.Count)
        <span class="plusicon1" onclick="GotoILSM()" id="addwo">
            <span title="Add ILSM"><i class="fa fa-plus" aria-hidden="true"></i></span>
        </span>
    </h4>
    <ul class="crx-scrollBar userhideshow">
        @foreach (var item in Model)
        {
            <li id="@item.TIlsmId" class="roundILSM">
                @item.IncidentId
            </li>
        }
    </ul>
</div>

<!---Fire Watch-->

<div class="adduserListpanel">
    <h4 class="roundsHeading" id="rilsm">
        <i class="fa fa-chevron-down"></i> Fire Watch (@Model.Count)
        <span class="plusicon1" onclick="GotoFireWatch()" id="addwo">
            <span title="Add Fire Watch"><i class="fa fa-plus" aria-hidden="true"></i></span>
        </span>
    </h4>
    <ul class="crx-scrollBar userhideshow">
        @foreach (var item in Model)
        {
            <li id="@item.TIlsmId" class="FireWatch">
                @item.IncidentId
            </li>
        }
    </ul>
</div>

<script>
    var roundId = $("#TRoundId").val()

    $("#rilsm").click(function () {
        $(".userhideshow").slideToggle('linear')
        $("#rinspections").find(".fa").toggleClass("rightarrow")
    })

    $(".roundILSM").click(function () {
        debugger
        var Id = $(this).attr("id");
        var urlaction ="@Url.Action("GetIlsmInspection", "Goal")";
        window.location.href = urlaction + "?tilsmId=" + Id + '&view=roundIlsm';
    });

    function GotoILSM() {
        var url = '@Url.Action("AddILSM", "ILSM")' //CRxUrls.Round_RoundIssues + "?rId=" + roundId
        debugger
        window.location.href = url + "?rId=" + roundId;
    }


       function GotoFireWatch() {
        var url = '@Url.Action("ManageFireWatchSchedules", "FireWatch")'
        window.location.href = url + "?rId=" + roundId;
    }

</script>
